        "controller.animation.5fs_cb.toggle_light": {
            "initial_state": "light_off",
            "states": {
                "light_off": {
                    "on_entry": [
                        "/fill ~1~~1 ~-1~~-1 air [] replace minecraft:light_block [\"block_light_level\": 15]"
                    ],
                    "on_exit": [
                        "/execute as @s[type=5fs_cb:lamp_cow,c=1,r=1] run playsound mob.cow.say @p ~~~",
                        "/execute as @s[type=5fs_cb:lamp_parrot,c=1,r=1] run playsound  mob.parrot.idle @p ~~~",
                        "/execute as @s[type=5fs_cb:lamp_end,c=1,r=1] run playsound  block.end_portal_frame.fill @p ~~~",
                        "/execute as @s[type=5fs_cb:strider_lamp,c=1,r=1] run playsound  mob.strider.tempt @p ~~~",
                        "/execute as @s[type=5fs_cb:lamp_warden,c=1,r=1] run playsound  power.on.sculk_sensor @p ~~~",
                        "/execute as @s[type=5fs_cb:lamp_guardian,c=1,r=1] run playsound  mob.elderguardian.hit @p ~~~",
                        "/execute as @s[type=5fs_cb:lamp_creeper,c=1,r=1] run playsound  random.fuse @p ~~~",
                        "/execute as @s[type=5fs_cb:fridge_iron_golem,c=1,r=1] run playsound  mob.irongolem.hit @p ~~~",
                        "/execute as @s[type=5fs_cb:chandelier_glow_squid,c=1,r=1] run playsound bubble.up @p ~~~",
                        "/execute as @s[type=5fs_cb:blaze_chandelier,c=1,r=1] run playsound mob.blaze.shoot @p ~~~"
                    ],
                    "transitions": [
                        {
                            "light_on": "q.is_sheared"
                        }
                    ]
                },
                "light_on": {
                    "on_entry": [
                        "/setblock ~~~ minecraft:light_block [\"block_light_level\": 15]"
                    ],
                    "on_exit": [
                        "/execute as @s[type=5fs_cb:lamp_cow,c=1,r=1] run playsound mob.cow.say @p ~~~",
                        "/execute as @s[type=5fs_cb:lamp_parrot,c=1,r=1] run playsound  mob.parrot.idle @p ~~~",
                        "/execute as @s[type=5fs_cb:lamp_end,c=1,r=1] run playsound  block.end_portal_frame.fill @p ~~~",
                        "/execute as @s[type=5fs_cb:strider_lamp,c=1,r=1] run playsound  mob.strider.tempt @p ~~~",
                        "/execute as @s[type=5fs_cb:lamp_warden,c=1,r=1] run playsound  power.on.sculk_sensor @p ~~~",
                        "/execute as @s[type=5fs_cb:lamp_guardian,c=1,r=1] run playsound  mob.elderguardian.hit @p ~~~",
                        "/execute as @s[type=5fs_cb:lamp_creeper,c=1,r=1] run playsound  extinguish.candle @p ~~~",
                        "/execute as @s[type=5fs_cb:fridge_iron_golem,c=1,r=1] run playsound  mob.irongolem.hit @p ~~~",
                        "/execute as @s[type=5fs_cb:chandelier_glow_squid,c=1,r=1] run playsound bubble.up @p ~~~",
                        "/execute as @s[type=5fs_cb:blaze_chandelier,c=1,r=1] run playsound mob.blaze.shoot @p ~~~"
                    ],
                    "transitions": [
                        {
                            "light_off": "!q.is_sheared"
                        }
                    ]
                }
            }
        },
