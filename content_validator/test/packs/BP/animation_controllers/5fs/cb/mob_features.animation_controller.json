        "controller.animation.5fs_cb.clear_inv": {
            "states": {
                "default": {
                    "transitions": [
                        {
                            "count_down": "q.is_sheared"
                        }
                    ],
                    "on_entry": [
                        "/clear @e[type=5fs_cb:trash_enderman,c=1,r=5]",
                        "/playsound mob.endermen.portal @p"
                    ]
                },
                "count_down": {
                    "transitions": [
                        {
                            "default": "!q.is_sheared"
                        }
                    ]
                }
            }
        },
