    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.5fs_cb.center_furniture": {
            "states": {
                "default": {
                    "transitions": [
                        {
                            "table": "q.has_any_family('mob_table') && q.is_baby"
                        },
                        {
                            "table": "q.has_any_family('fridge_ig') && q.is_baby"
                        }
                    ]
                },
                "table": {
                    "on_entry": [
                        "/execute as @e[family=mob_table,c=1,r=1] align xyz run tp @s ~~~"
                    ],
                    "transitions": [
                        {
                            "default": "!q.is_baby"
                        }
                    ]
                },
                "fridge": {
                    "on_entry": [
                        "/execute as @e[family=fridge_ig,c=1,r=1] align xyz run tp @s ~~~"
                    ],
                    "transitions": [
                        {
                            "default": "!q.is_baby"
                        }
                    ]
                }
            }
        },
