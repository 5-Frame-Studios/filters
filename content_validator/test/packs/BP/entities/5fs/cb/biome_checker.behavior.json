    "events": {
      "5fs:instant_despawn": {
        "add": {
          "component_groups": ["5fs:instant_despawn"]
        }
      },
      "minecraft:entity_spawned": {
        "sequence": [
          {
            //Watchtower
            "filters": {
              "test": "has_biome_tag",
              "value": "taiga"
            },
            "set_property": {
              "5fs.cb.itembehavior:biome_type": 0
            }
          },
          {
            //Barn
            "filters": [
              {
                "all_of": [
                  {
                    "test": "has_biome_tag",
                    "value": "savanna"
                  }
                ]
              }
            ],
            "set_property": {
              "5fs.cb.itembehavior:biome_type": 1
            }
          },
          {
            "filters": {
              "any_of": [
                {
                  "test": "has_biome_tag",
                  "value": "stone_beach"
                },
                {
                  "test": "has_biome_tag",
                  "value": "beach"
                }
              ]
            },
            "set_property": {
              "5fs.cb.itembehavior:biome_type": 2
            }
          },
