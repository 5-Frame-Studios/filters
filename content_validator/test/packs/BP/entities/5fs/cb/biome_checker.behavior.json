                {
                  "test": "has_biome_tag",
                  "value": "swamp"
                },
                {
                  "test": "has_biome_tag",
                  "value": "mangrove_swamp"
                }
              ]
            },
            "set_property": {
              "5fs.cb.itembehavior:biome_type": 6
            }
          },
