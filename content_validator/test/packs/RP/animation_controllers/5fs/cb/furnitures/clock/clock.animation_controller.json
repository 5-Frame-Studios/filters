{
    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.5fs_cb.clock": {
            "states": {
                "default": {
                    "animations": [
                        "idle"
                    ],
                    "transitions": [
                        {
                            "toggle": "q.is_sheared"
                        }
                    ],
                    "blend_transition": 0.2
                },
                "toggle": {
                    "animations": [
                        "toggle"
                    ],
                    "transitions": [
                        {
                            "default": "q.any_animation_finished && !q.is_sheared"
                        }
                    ]
                }
            }
        }
    }
}