		"animation.5fs_cb.structure.origin": {
			"loop": true,
			"bones": {
				"root": {
					"rotation": [0, "math.round(query.body_y_rotation/90)*90-query.body_y_rotation", 0]
				}
			}
		},
		"animation.5fs_cb.structure.90_degree": {
			"loop": "hold_on_last_frame",
			"bones": {
				"root": {
					"rotation": [0, 90, 0]
				}
			}
		},
		"animation.5fs_cb.structure.180_degree": {
			"loop": "hold_on_last_frame",
			"bones": {
				"root": {
					"rotation": [0, 180, 0]
				}
			}
		},
		"animation.5fs_cb.structure.270_degree": {
			"loop": "hold_on_last_frame",
			"bones": {
				"root": {
					"rotation": [0, 270, 0]
				}
			}
